<!DOCTYPE html>
<html>
<head>
  <title>MultiWeather</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= stylesheet_link_tag    'css/weather-icons', media: 'all' %>
  <%= stylesheet_link_tag    '/font', media: 'all' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class='container'>
    <%= render 'layouts/navigation' %>
    <%= render 'layouts/display_msg' %>
    <%= yield %>
  </div>

</body>

</html>
